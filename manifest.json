{
  "manifest_version": 3,
  "name": "USCF MSA Visual Fix",
  "version": "1.0",
  "description": "Fixes visual alignment on USCF MSA pages",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.uschess.org/msa/MbrDtl*"],
      "js": ["content.js"]
    }
  ]
}